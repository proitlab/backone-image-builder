
config rpcd
	option socket '/var/run/ubus/ubus.sock'
	option timeout '30'

config login
	option username 'root'
	option password '$p$root'
	list read '*'
	list write '*'

config login
	option username 'support'
	option password '$p$support'
	option timeout '300'
	list read 'luci-app-commands'
	list read 'luci-app-firewall'
	list read 'luci-app-sqm'
	list read 'luci-base'
	list read 'luci-base-network-status'
	list read 'luci-mod-network-config'
	list read 'luci-mod-network-dhcp'
	list read 'luci-mod-network-diagnostics'
	list read 'luci-mod-status-channel_analysis'
	list read 'luci-mod-status-firewall'
	list read 'luci-mod-status-index'
	list read 'luci-mod-status-index-dhcp'
	list read 'luci-mod-status-index-dsl'
	list read 'luci-mod-status-index-wifi'
	list read 'luci-mod-status-logs'
	list read 'luci-mod-status-realtime'
	list read 'luci-mod-status-routes'
	list read 'luci-mod-system-cron'
	list read 'luci-mod-system-reboot'
	list read 'luci-mod-system-uhttpd'
	list read 'unauthenticated'
	list write 'luci-app-firewall'
	list write 'luci-app-sqm'
	list write 'luci-base'
	list write 'luci-base-network-status'
	list write 'luci-mod-network-config'
	list write 'luci-mod-network-dhcp'
	list write 'luci-mod-network-diagnostics'
	list write 'luci-mod-status-channel_analysis'
	list write 'luci-mod-status-firewall'
	list write 'luci-mod-status-index'
	list write 'luci-mod-status-index-dhcp'
	list write 'luci-mod-status-index-dsl'
	list write 'luci-mod-status-index-wifi'
	list write 'luci-mod-status-logs'
	list write 'luci-mod-status-realtime'
	list write 'luci-mod-status-routes'
	list write 'luci-mod-system-cron'
	list write 'luci-mod-system-reboot'
	list write 'luci-mod-system-uhttpd'

